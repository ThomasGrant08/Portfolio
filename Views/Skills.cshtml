@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Skills>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}

@Html.Partial("Header")

<div class="Block SkillBlock Page">
	<div class="Breakpoint-Wrap">
		<div class="Text">

		</div>
		<div class="SkillContainer" id="accordion">
			@foreach (Skill skill in Model.Children())
			{
				@Html.Partial("Skills Card", (IPublishedContent)skill)
			}
		</div>
	</div>
</div>